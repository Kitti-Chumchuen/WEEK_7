#include <stdio.h>
#include<ctype.h>
#include<string.h>

int main () {
  char a[100],c;
  int range = 0,b;
  printf("Enter a string : ");
  scanf("%[^\n]",a);
  range = strlen(a);
  for(int i=0; i<range; i++){
    c = a[i];
    b = (int)c;
    if(b >= 65 && b <= 90){
      a[i] = tolower(c);
    }
    else if(b >= 97 && b <= 122){
      a[i] = toupper(c);
    }
  }
  printf("%s",a);
  return 0;
}
